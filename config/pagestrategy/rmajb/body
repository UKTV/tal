<script type="text/javascript">
window.ajb.registerListener(ajb.MessageType.NAVIGATION, function(message) {
    if (message.value === 'back') {
        var evt = {
            keyCode: 166,
            preventDefault: function(){}
        };
        document.onkeyup(evt);
        return true;
    }
    return false;
});
</script>
